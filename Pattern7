Enter no. of rows: 3
A B C 
B C D 
C D E 

#include<iostream>
using namespace std;

int main(){
    int n;
    cout<<"Enter no. of rows: ";
    cin>>n;

    int row=1;
    while(row<=n){
        char ch=64+row;
        int col=1;
        while(col<=n){
            cout<<ch<<" ";
            ch = ch+1;
            col = col+1;
        }
        cout<<endl;
        row = row+1;
    }
}
