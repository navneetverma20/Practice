Enter no. of rows: 3
1 2 3 
4 5 6 
7 8 9 

#include<iostream>
using namespace std;

int main(){
    int n, i=1, count=1;
    cout<<"Enter no. of rows: ";
    cin>>n;

    while(i<=n){
        int j=1;
        while(j<=n){
            cout<<count<<" ";
            count = count+1;
            j= j+1;
        }
        cout<<endl;
        i = i+1;
    }
}
