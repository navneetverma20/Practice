Enter no. of rows: 3
A A A 
B B B 
C C C 

#include<iostream>
using namespace std;

int main(){
    int n;
    cout<<"Enter no. of rows: ";
    cin>>n;

    int row=1;
    char ch=65;
    while(row<=n){
        int col=1;
        while(col<=n){
            cout<<ch<<" ";
            col= col+1;
        }
        cout<<endl;
        ch = ch+1;
        row = row+1;
    }
}
